{
  "name": "clerk-supabase-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "cleanup": "node scripts/cleanup.js",
    "scan:dead-files": "node scripts/scan-dead-files.js",
    "prepare": "husky install",
    "security:audit": "bash scripts/security-audit.sh",
    "security:scan": "gitleaks detect --source . --config .gitleaks.toml --verbose",
    "security:setup": "bash scripts/setup-husky.sh",
    "precommit": "npm run security:scan && tsc --noEmit && npm run lint",
    "predeploy": "npm run security:audit && npm run build"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.44",
    "@ai-sdk/openai": "^2.0.77",
    "@ai-sdk/react": "^2.0.106",
    "@clerk/nextjs": "^5.0.0",
    "@supabase/supabase-js": "^2.39.0",
    "ai": "^5.0.106",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "mammoth": "^1.6.0",
    "next": "^14.2.33",
    "pdf-parse": "^1.1.1",
    "razorpay": "^2.9.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.6.0",
    "svix": "^1.15.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "husky": "^9.1.7",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
